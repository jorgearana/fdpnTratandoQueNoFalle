@model InscripcionNatacion.ViewModels.Torneo.UnEventoViewModel
@{
    ViewBag.Title = "Listados Por Prueba";
}



<div class="wrapper wrapper-content  animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <h2>Listados de inscritos por prueba</h2>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-6">
                        @Html.DropDownListFor(model => model.EventoId, new SelectList(Model.UnosEventos, "id", "Nombre"), "Escoja una prueba", new { @class = "form-control" })
                        <div id="DatosDelEvento"></div>
                    </div>
                   
                    <div class="col-lg-6">
                        <div id="InscritosAlEvento"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>