@model List<InscripcionNatacion.ViewModels.Inscrito.  ListarNadadoresParaSeleccionarlos>
    {
    ViewBag.Title = "InscribirNadador";
}
<!-- BEGIN CONTENT HEADER -->
<section class="content-header">
    <i class="fa fa-user-plus"></i>
    <span>Inscribir al deportista en las pruebas de su categoría</span>
</section>
<!-- END CONTENT HEADER -->
<!-- BEGIN MAIN CONTENT -->
<section class="content">
    <div class="row">
        <!-- BEGIN BASIC DATATABLES -->
        <div class="col-md-12">
            <div class="grid no-border">
                <div class="grid-header">
                    <i class="fa fa-television"></i>
                    <span class="grid-title">Relación de pruebas</span>
                    <div class="pull-right grid-tools">
                        <a data-widget="collapse" title="Collapse"><i class="fa fa-chevron-up"></i></a>
                        <a data-widget="reload" title="Reload"><i class="fa fa-refresh"></i></a>
                        <a data-widget="remove" title="Remove"><i class="fa fa-times"></i></a>
                    </div>
                </div>
                <div class="grid-body">

                    <table id="dataTables1" class="table table-hover display" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Sesión</th>
                                <th>Prueba</th>
                                <th>Distancia</th>
                                <th>Estilo</th>
                                <th>Edad</th>
                                <th>Marca</th>
                                <th>MM larga</th>
                                <th>MM corta</th>
                                <th>Torneo</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var @item in Model)
                            {
                                <tr>
                                    <td>
                                        @item.sesiones.Sess_ptr
                                    </td>
                                    <td>@item.evento.Event_no</td>
                                    <td>@item.evento.Event_dist </td>
                                    <td>@item.evento.Event_stroke</td>
                                    <td>@item.evento.Low_age - @item.evento.High_Age</td>
                                    @if (@item.tiempos != null)
                                    {
                                        <td>@item.tiempos.SCORE</td>
                                    }
                                    else
                                    {
                                        <td>NT</td>
                                    }

                                    <td>
                                        @item.MMLargaString
                                    </td>
                                    <td>
                                        @item.MMCortaString
                                    </td>
                                    @if (@item.tiempos != null)
                                    {
                                        <td>
                                            <a target="_blank" href="http://fdpn.jorgearananeyra.com/Home/ResultadosPorclubes?meetId=7722">@item.tiempos.MEET1.MName</a>
                                        </td>
                                    }
                                    else
                                    {
                                        <td>Sin evento</td>
                                    }
                                    
                                    
                                </tr>
                            }
                        </tbody>
                    </table>


                </div>
            </div>
        </div>
        <!-- END BASIC DATATABLES -->
    </div>
</section>
<!-- END MAIN CONTENT -->