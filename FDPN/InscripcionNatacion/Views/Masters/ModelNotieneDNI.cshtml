@model InscripcionNatacion.ViewModels.Inscrito.ListadoInscritoViewModel
@{
  
    FDPN.Models.SetupTorneo setup = ViewBag.setup;
    FDPN.Models.Rol rol = Session["Rol"] as FDPN.Models.Rol;
}

<div class="modal-content">
    <div class="modal-header">
        <h2 class="modal-title"> No encontramos el DNI</h2>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-lg-12">
                <div class="widget-head-color-box navy-bg p-lg text-center">
                    <div class="m-b-md">
                        <h2 class="font-bold no-margins">
                           Si es la primera vez que va a participar en un torneo oficial presione este botón
                            <button class="btnInscribirSinDNI btn btn-warning">Inscribir sin tiempos</button>
                        </h2>
                        <h3 class="font-bold no-margins">
                            Puede comunicarse al whatsapp 980599023 indicando el nombre del deportista para buscar sus tiempos.
                        </h3>
                    </div>
                </div>
            </div>
           
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
    </div>

    <div style="visibility:hidden">
        <div id="Piscina">@Model.Piscina</div>
        <div id="InscripcionId">@Model.afiliado.InscripcionId</div>
        <div id="MeetId">@Model.MeetId</div>
        <div id="YaestaInscrito">@Model.YaestaInscrito</div>
    </div>
</div>
<div style="visibility:hidden" id="IdDelAfiliado">@Model.afiliado.InscripcionId</div>
