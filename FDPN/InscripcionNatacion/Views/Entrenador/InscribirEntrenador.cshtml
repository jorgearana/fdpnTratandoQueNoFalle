@model InscripcionNatacion.ViewModels.Entrenador.EscogerEntrenadorViewModel
@{
    ViewBag.Title = "Inscribir Entrenador en este torneo";
}



<div class="wrapper wrapper-content  animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-title">
                    <h2>Inscribir Entrenador en este torneo</h2>
                </div>
                <br />
                <div class="row">
                    <div class="col-lg-4">
                        @Html.DropDownListFor(model => model.EventoId, new SelectList(Model.UnosEventos, "id", "Nombre"), "Escoja una prueba", new { @class = "form-control" })
                        <div id="DatosDelEvento"></div>
                    </div>

                    <div class="col-lg-8">
                        <div id="InscritosAlEvento"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>