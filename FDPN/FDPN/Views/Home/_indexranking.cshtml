@model List<FDPN.Models.RESULTS>

@{
    string prueba;
    if (Model[0].COURSE == "S")
    {
        prueba = @Model[0].DISTANCE + " " + @Model[0].STROKE + " - " + @Model[0].Athlete1.Sex + " Piscina larga";
    }
    else
    {
        prueba = @Model[0].DISTANCE + " " + @Model[0].STROKE + " - " + @Model[0].Athlete1.Sex + " Piscina larga";
    }

}


<div class="card card-pricing card-raised">
    <div class="col-md-8 ml-auto mr-auto">
        <h2 class="title ">Top 10</h2>
        <h4 class="title ">@prueba  </h4>

    </div>

    <table class="table ">
        <thead>
            <tr>
                <th>#</th>
                <th>Nombre</th>
                <th>Tiempo</th>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@(i+1)</td>
                    <td><span>@Model[i].Athlete1.First.ToUpper()</span><span>  @Model[i].Athlete1.Last.ToUpper()</span> </td>
                    <td>
                        @Model[i].SCORE
                    </td>
                    <td>@Model[i].MEET1.Start.ToString("dd/MMM/yyyy")</td>
                </tr>

            }
        </tbody>
    </table>

</div>