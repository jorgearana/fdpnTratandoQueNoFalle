@model FDPN.ViewModels.Resultados.ResultadoDeUnTorneoViewModel

@{
    ViewBag.Title = "Resultado Por clubes";
}


<section class="site-homepage__section site-athletedetails">

    <div class="wrapper site-homepage__pushup">

        <header class="site-homepage__section__header">            
                <h2 class="site-title site-title--ultra site-title--ultra--primary">Resultados del club @Model.club.TName </h2>
                <p class="u-type--medium u-type--delta u-spc-bottom--med">en el torneo @Model.torneo.MName</p>
        </header>
        <div class="site-homepage__section__content">
            @*<h2 class="u-type--gamma u-spc-bottom--med">Listado de resultados del club</h2>*@
            <div class="table">
                <table class="table__data">
                    <thead>
                        <tr>
                           
                            <th>Puesto</th>
                            <th>Tiempo</th>
                            <th>Distancia</th>
                            <th>Estilo</th>
                            <th>Deportista</th>
                            <th>Sexo</th>
                            <th>Edad</th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.resultado)
                        {
                            <tr>
                               
                                <td>@item.PLACE</td>
                                <td>@item.SCORE</td>
                                <td>@item.DISTANCE</td>
                                <td>@item.STROKE</td>
                                <td>@Html.ActionLink(@item.Athlete1.First + " " + @item.Athlete1.Last, "Nadador", "Resultados", new { id = item.AthleteId }, null)</td>
                                <td>@item.Athlete1.Sex</td>
                                <td>@item.Athlete1.Age</td>

                            </tr>
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <div class="espaciadogrande"></div>
</section>
